{
  "name": "helper-utility",
  "version": "0.0.0-development",
  "description": "helper utility",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:jameswlane/helper-utility.git"
  },
  "keywords": [
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/jameswlane/helper-utility/issues"
  },
  "homepage": "https://github.com/jameswlane/helper-utility#readme",
  "scripts": {
    "eslint": "eslint src test",
    "eslint:fix": "eslint --fix src test",
    "test": "jest --coverage",
    "test:helper": "babel-node index.js",
    "verify": "yarn verify:npmvet && yarn verify:david && yarn verify:nsp && yarn eslint && yarn test",
    "verify:npmvet": "npmvet --renderer ci --strict",
    "verify:david": "david",
    "verify:nsp": "nsp check --output summary",
    "verify:check": "depcheck",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "James W. Lane",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-preset-es2017": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chalk": "2.3.0",
    "coveralls": "3.0.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.12.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "jest": "21.2.1",
    "regenerator-runtime": "0.11.0",
    "semantic-release": "8.2.0",
    "sinon": "4.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
